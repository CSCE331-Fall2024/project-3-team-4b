{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "DB_USER": "@db_user",
    "DB_HOST": "@db_host",
    "DATABASE": "@database",
    "DB_PASS": "@db_pass",
    "PORT": "5432"
  }
}